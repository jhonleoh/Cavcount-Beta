{
  "name": "cavcount",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "npm run generate-metadata && SKIP_ESLINT_CHECK=true SKIP_TYPE_CHECK=true NEXT_DISABLE_TYPESCRIPT_CHECK=true NEXT_SKIP_LINT=true next build --no-lint",
    "build:static": "npm run generate-metadata && node build-wrapper.js && next export",
    "generate-metadata": "node scripts/generate-metadata.js",
    "postbuild": "cp public/*.html out/",
    "start": "next start -H 0.0.0.0",
    "lint": "eslint src/*",
    "format": "biome format --write ./src"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "biome": "^0.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.309.0",
    "next": "^14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^5.0.4",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
